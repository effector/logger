{
  "name": "effector-logger",
  "version": "0.0.0-real-version-will-be-set-on-ci",
  "main": "./index.js",
  "module": "./index.mjs",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "repository": "github:effector/logger",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite ./usage",
    "build": "rm -rf ./dist && ts-node ./scripts/build.ts",
    "build:integration": "LIB_NAME=@effector/logger yarn build",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "echo no tests yet",
    "commit": "git-cz",
    "prepublish": "yarn build"
  },
  "packageManager": "pnpm@7.11.0",
  "peerDependencies": {
    "effector": "^22.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/babel__core": "^7.1.19",
    "@types/detect-node": "^2.0.0",
    "@types/node": "^13.7.1",
    "@types/set-value": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-tester": "^10.0.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "effector": "^22.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.4.0",
    "typescript": "=4.3.5",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "detect-node": "2.0.4",
    "effector-inspector": "^0.10.3",
    "just-debounce-it": "^1.1.0",
    "set-value": "4.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "files": [
    "dist",
    "attach.js",
    "attach.js.map",
    "attach.mjs",
    "attach.mjs.map",
    "index.js",
    "index.js.map",
    "index.mjs",
    "index.mjs.map",
    "inspector.js",
    "inspector.js.map",
    "inspector.mjs",
    "inspector.mjs.map",
    "babel-plugin.js",
    "macro.js",
    "macro.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.mjs"
    },
    "./index.mjs": "./index.mjs",
    "./attach": {
      "import": "./attach.mjs",
      "require": "./attach.js",
      "default": "./attach.mjs"
    },
    "./inspector": {
      "import": "./inspector.mjs",
      "require": "./inspector.js",
      "default": "./inspector.mjs"
    },
    "./babel-plugin": {
      "default": "./babel-plugin.js"
    },
    "./macro": {
      "default": "./macro.js"
    }
  },
  "typesVersions": {
    "*": {
      "attach": [
        "dist/attach.d.ts"
      ]
    }
  }
}
